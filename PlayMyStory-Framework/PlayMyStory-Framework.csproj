<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <BaseIntermediateOutputPath>..\Cache\Framework\</BaseIntermediateOutputPath>
    <TargetFramework>net7.0</TargetFramework>
    <RootNamespace>PlayMyStory_Framework</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>PlayMyStory</Title>
    <Authors>Nico Jablinski</Authors>
    <Copyright>Nico Jablinski</Copyright>
    <PackageProjectUrl>https://github.com/CoffeeNeko/PlayMyStory/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/CoffeeNeko/PlayMyStory/</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <BaseOutputPath>..\Builds\Framework\</BaseOutputPath>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <DocumentationFile>..\Documentation\Framework.XML</DocumentationFile>
    <BaseIntermediateOutputPath>..\Cache\Framework\</BaseIntermediateOutputPath>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="obj\**" />
    <EmbeddedResource Remove="obj\**" />
    <None Remove="obj\**" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\PlayMyStory-Settings\PlayMyStory-Settings.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Interface\" />
  </ItemGroup>

</Project>
